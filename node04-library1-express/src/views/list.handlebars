<a href="/book/create">Create new book</a>
<h3>Books:</h3>

<table class="books">
    <thead>
      <tr>
          <th>ID</th>
          <th>Title</th>
          <th>Edit</th>
          <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      {{#each books}}
       <tr></tr>
          <th>{{this.id}}</th>
          <th>{{this.title}}</th>
          <th>Edit</th>
          <th>Delete</th>
      </tr>
      {{/each}}
    </tbody>
</table>